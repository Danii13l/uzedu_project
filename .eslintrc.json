{
  "extends": [
    "next/babel",
    "next/core-web-vitals"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": [
      "warn",
      "always"
    ],
    "no-var": [
      "error"
    ],
    "no-console": [
      "warn"
    ],
    "no-unused-vars": "warn"
  }
}