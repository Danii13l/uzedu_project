{
  "extends": [
    "next/core-web-vitals"
    // "next/babel"
  ],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": [
      "warn",
      "always"
    ],
    "@next/next/no-img-element": "off",
    "no-var": [
      "error"
    ],
    "no-console": [
      "warn"
    ],
    "no-unused-vars": "warn"
  }
}